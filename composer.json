{
    "name": "dispositiontools/craft-minical",
    "description": "Show entries in a calendar layout",
    "type": "craft-plugin",
    "version": "4.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "minical"
    ],
    "support": {
        "docs": "https://github.com/dispositiontools/minical/blob/master/README.md",
        "issues": "https://github.com/dispositiontools/minical/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Disposition Tools",
            "homepage": "https://www.disposition.tools"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0"
    },
    "config": {
      "allow-plugins": {
        "craftcms/plugin-installer": true,
        "yiisoft/yii2-composer": true
      },
      "optimize-autoloader": true,
      "sort-packages": true
    },
    "autoload": {
        "psr-4": {
          "dispositiontools\\minical\\": "src/"
        }
    },
    "extra": {
        "name": "Minical",
        "handle": "minical",
        "developer": "Disposition Tools",
        "developerUrl": "https://www.disposition.tools",
        "documentationUrl": "https://disposition.tools/craft-plugins/mini-cal",
        "changelogUrl": "https://raw.githubusercontent.com/dispositiontools/minical/master/CHANGELOG.md",
        "components": {
            "layout": "dispositiontools\\minical\\services\\Layout"
        },
        "class": "dispositiontools\\minical\\Minical"
    }
}
